    <ReduceAffliction identifier="stun" strength="100" />
    <ReduceAffliction identifier="eyibugspawnner" strength="100" />
  </StatusEffect>
</Effect>
<!--最终阶段，眩晕并呕吐，达到100时开始转变-->
<Effect minstrength="90" maxstrength="100"
  maxvitalitydecrease="0"
  strengthchange="1"
  minbodytint="195,195,195,150"
  maxbodytint="195,195,195,255">
  <StatusEffect target="Character">
    <Affliction identifier="stun" amount="1" />
    <Affliction identifier="nausea" amount="3" />
  </StatusEffect>
  <StatusEffect target="Character" comparison="and">
    <Conditional isdead="false" />
    <Conditional eyiinfection="eq 94.0" />
    <ParticleEmitter particle="bloodsplash" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="blackblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="waterblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="tsm_redwaterblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
  </StatusEffect>
  <StatusEffect target="Character" comparison="and">
    <Conditional isdead="false" />
    <Conditional eyiinfection="eq 96.0" />
    <ParticleEmitter particle="bloodsplash" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="blackblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="waterblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="tsm_redwaterblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
  </StatusEffect>
  <StatusEffect target="Character" comparison="and">
    <Conditional isdead="false" />
    <Conditional eyiinfection="eq 98.0" />
    <ReduceAffliction identifier="tsm_stress" strength="100" />
    <ReduceAffliction identifier="stun" strength="0.5" />
    <ParticleEmitter particle="bloodsplash" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="blackblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="waterblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
    <ParticleEmitter particle="tsm_redwaterblood" particlespersecond="100" scalemin="1" scalemax="2" velocitymin="0" velocitymax="40" anglemin="0" anglemax="360" />
  </StatusEffect>
  <StatusEffect target="Character" comparison="and">
    <Conditional isdead="true" setvalue="true"/>
    <Conditional eyiinfection="eq 100.0" />
    <Affliction identifier="internaldamage" amount="17" />
    <ReduceAffliction identifier="stun" strength="2" />
