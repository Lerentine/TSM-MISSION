<!--0-10 死亡产生破皮者-->
<Effect minstrength="0" maxstrength="10"
  maxvitalitydecrease="0"
  strengthchange="0"
  minbodytint="195,195,195,0"
  maxbodytint="195,195,195,150">
  <StatusEffect target="Character">
    <Affliction identifier="eyiskinbreakerspawnner" amount="5"/>
  </StatusEffect>
</Effect>
<!--10-40 死亡产生抱身者-->
<Effect minstrength="10" maxstrength="40"
  maxvitalitydecrease="0"
  strengthchange="1"
  minbodytint="195,195,195,0"
  maxbodytint="195,195,195,150">
  <StatusEffect target="Character" comparison="and" type="OnDeath" delay="3">
    <ReduceAffliction identifier="eyiskinbreakerspawnner" strength="100" />
    <Conditional eyiinfection="gte 10.0" />
    <Conditional eyiinfection="lte 30.0" />
    <SpawnCharacter speciesname="Eyi_hugger" count="1" spread="0" />
  </StatusEffect>
</Effect>
<!--40-45 死亡产生变体-->
<Effect minstrength="40" maxstrength="45"
  maxvitalitydecrease="0"
  strengthchange="1"
  minbodytint="195,195,195,0"
  maxbodytint="195,195,195,150">
  <StatusEffect target="Character" comparison="and" type="OnDeath" delay="3">
    <Conditional eyiinfection="gte 40.0" />
    <Conditional eyiinfection="lte 45.0" />
    <SpawnCharacter speciesname="eyivariant" count="1" spread="0" />
  </StatusEffect>
</Effect>
<!--45-75 死亡产生吃骨者-->
<Effect minstrength="45" maxstrength="75"
  maxvitalitydecrease="0"
  strengthchange="1"
  minbodytint="195,195,195,0"
  maxbodytint="195,195,195,150">
  <StatusEffect target="Character" comparison="and" type="OnDeath" delay="3">
    <Conditional eyiinfection="gte 45.0" />
    <Conditional eyiinfection="lte 75.0" />
    <SpawnCharacter speciesname="boneeater" count="1" spread="0" />
  </StatusEffect>
</Effect>
<!--75~90，短暂的可活动阶段-->
