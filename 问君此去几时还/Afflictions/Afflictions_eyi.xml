<!--0~30缓慢增长压力，不可检测-->
<Effect minstrength="0" maxstrength="30"
  maxvitalitydecrease="0"
  strengthchange="0.3"
  minbodytint="195,195,195,0"
  maxbodytint="195,195,195,150">
  <StatusEffect target="Character">
    <Affliction identifier="tsm_stress" amount="0.3" />
  </StatusEffect>
</Effect>
<!--30~60快速增长压力直至压力达60-->
<Effect minstrength="30" maxstrength="60"
  maxvitalitydecrease="0"
  strengthchange="1"
  minbodytint="195,195,195,150"
  maxbodytint="195,195,195,200">
  <StatusEffect target="Character" comparison="and">
    <Conditional tsm_stress="lt 60" />
    <Affliction identifier="tsm_stress" amount="1.7" />
  </StatusEffect>
  <StatusEffect target="Character">
    <Conditional tsm_stress="gte 60" />
    <Affliction identifier="tsm_stress" amount="1.5" />
  </StatusEffect>
  <StatusEffect target="Character">
    <Affliction identifier="eyibugspawnner" amount="5" />
  </StatusEffect>
</Effect>
<!--60~80倒地呕吐-->
<Effect minstrength="60" maxstrength="80"
  maxvitalitydecrease="0"
  strengthchange="0.7"
  minbodytint="195,195,195,200"
  maxbodytint="195,195,195,255">
  <StatusEffect target="Character">
    <Affliction identifier="stun" amount="0.35" />
    <Affliction identifier="nausea" amount="1" />
  </StatusEffect>
</Effect>
<!--80以后进入第二阶段，无法治疗-->
<Effect minstrength="80" maxstrength="100"
  maxvitalitydecrease="0"
  strengthchange="0.3"
  minbodytint="195,195,195,200"
  maxbodytint="195,195,195,255">
  <StatusEffect target="Character" setvalue="true">
    <Affliction identifier="eyiinfection" amount="30" />
    <ReduceAffliction identifier="tsm_virusstage" strength="100" />
    <ReduceAffliction identifier="tsm_stress" strength="100" />
    <ReduceAffliction identifier="psychosis" strength="100" />
  </StatusEffect>
</Effect>
<icon texture="%ModDir%/Afflictions/Afflictionicons.png" sourcerect="128,1,122,119" color="255,0,0,255" origin="0,0" />
</Affliction>
