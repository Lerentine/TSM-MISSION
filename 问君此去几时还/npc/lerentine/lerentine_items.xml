        <Conditional Condition="gt 40" />
        <!--武器耐久大于40-->
        <Affliction identifier="tsm_melee_defense_buff" amount="9" />
        <!--招架9-->
        <SpawnItem identifier="collider_sword" spawnposition="This" />
        <!--生成碰撞箱-->
      </StatusEffect>
      <!--攻击前摇、后摇时格挡-->
      <!--格挡消耗耐久,最低到20-->
      <StatusEffect type="OnSecondaryUse" target="This" Condition="-50" comparison="And">
